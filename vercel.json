{
    "rewrites": [
      {
        "source": "/portfolio/:path*",
        "destination": "/:path*"
      }
    ],
    "redirects": [
      {
        "source": "/portfolio",
        "destination": "/portfolio/",
        "permanent": true
      }
    ],
    "cleanUrls": true,
    "trailingSlash": true
  }